generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Behavior {
  id         Int       @id @default(autoincrement())
  event      String?   @db.VarChar(254)
  target     String?   @db.VarChar(254)
  timestamp  DateTime? @db.Date @default(now())
  userAgent  String?   @db.VarChar(254)
  pageUrl    String?   @db.VarChar(254)
  extra      String?
  createdAt  DateTime? @db.Date @default(now())
  project_id String?   @db.VarChar(255)

  @@index([project_id], map: "idx_behavior_project")
}

model Blank_Screen {
  id          Int       @id @default(autoincrement())
  isBlank     String?   @db.VarChar(254)
  checkPoints String?
  timestamp   DateTime? @db.Date @default(now())
  pageUrl     String?   @db.VarChar(254)
  extra       String?
  createdAt   DateTime? @db.Date @default(now())
  project_id  String?   @db.VarChar(255)

  @@index([project_id], map: "idx_blank_project")
  @@map("Blank Screen")
}

model Error {
  id         Int       @id @default(autoincrement())
  errorType  String?   @db.VarChar(254)
  message    String?
  stack      String?
  timestamp  DateTime? @db.Date @default(now())
  pageUrl    String?   @db.VarChar(254)
  userAgent  String?   @db.VarChar(254)
  extra      String?
  createdAt  DateTime? @db.Date @default(now())
  project_id String?   @db.VarChar(255)

  @@index([project_id], map: "idx_error_project")
}

model Performance {
  id         Int       @id @default(autoincrement())
  loadTime   Int?
  domReady   Int?
  firstPaint Int?
  timestamp  DateTime? @db.Date @default(now())
  pageUrl    String?   @db.VarChar(254)
  extra      String?
  createdAt  DateTime? @db.Date @default(now())
  project_id String?   @db.VarChar(255)

  @@index([project_id], map: "idx_performance_project")
}

model Track_Event {
  id          BigInt    @id @default(autoincrement())
  project_id  String?   @db.VarChar(254)
  type        String?   @db.VarChar(254)
  event_name  String?   @db.VarChar(254)
  user_id     String?   @db.VarChar(254)
  page_url    String?
  sdk_version String?   @db.VarChar(254)
  ua          String?
  created_at  DateTime? @db.Date @default(now())
  raw_data    String?   @db.VarChar(254)

  @@map("Track Event")
}

model Users {
  id               Int       @id @default(autoincrement())
  username         String?   @db.VarChar(254)
  password         String?   @db.VarChar(254)
  createdAt        DateTime? @db.Date @default(now())
  updatedAt        DateTime? @db.Date @default(now())
  telephone_number BigInt?
  email            String?   @db.VarChar(254)
  name             String?   @db.VarChar(254)
  gender           String?   @db.VarChar(254)
  age              Int?
  bio              String?
  avatar           String?   @db.VarChar(254)
  
  // 关联项目
  projects         Project[]
}

model Project {
  id             Int       @id @default(autoincrement())
  name           String    @db.VarChar(254)
  url            String    @db.VarChar(254)
  apiKey         String    @db.VarChar(254) @unique
  type           String    @db.VarChar(254)
  description    String?
  status         String    @db.VarChar(254) @default("pending")
  monitorStatus  String    @db.VarChar(254) @default("enabled")
  userId         Int
  createdAt      DateTime  @db.Date @default(now())
  updatedAt      DateTime  @db.Date @default(now())
  
  // 关联用户
  user           Users     @relation(fields: [userId], references: [id])

  @@index([userId], map: "idx_project_user")
  @@index([apiKey], map: "idx_project_api_key")
}
